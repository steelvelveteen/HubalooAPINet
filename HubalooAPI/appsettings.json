{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft":"Information",
        "System":"Warning"
      },
      "Enrich": [ 
        "FromLogContext", 
        "WithMachineName", 
        "WithProcessId", 
        "WithThreadId" 
      ],
      "WriteTo": [
        { "Name": "Console" }
      ]
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": ""
}
